{
    "exclude": [
      "dist",
      "node_modules"
    ],
    "compilerOptions": {
      "strict": true,
      "strictNullChecks": true,
      "target": "es2017",
      "noImplicitAny": false,
      "module": "esnext",
      "moduleResolution": "node",
      "outDir": "dist",
      "sourceMap": true,
      "inlineSources": true,
      "jsx": "react"
    },
    "include": [
      "src/*"
    ]
}


  


  